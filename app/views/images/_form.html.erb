<div class="form">	
<%= form_for [collection, image], :html => {:multipart => true, :id => "images_form" } do |f| -%>
<%= render :partial => "shared/errors", :object => image.errors %>
<div class="content">
  <div class="line">
		<%= f.label :name %><%= f.text_field :name %>
	</div>
  <div class="line">
		<%= f.label :uploaded_file %><%= f.file_field :uploaded_file %>
	</div>
</div>
<%= render :partial => "shared/buttons", :locals => { :submit_text => submit_text, :form_id => "images_form" } %>
<% end -%>
</div>
